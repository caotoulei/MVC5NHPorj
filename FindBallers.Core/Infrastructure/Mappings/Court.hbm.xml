<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="CMS.Core.Domain.Models" assembly="CMS.Core">

  <class name="Court" table="Location">

    <id name="Id">
      <!--<column name="Id" sql-type="uniqueidentifier" not-null="true"/>-->
      <generator class="guid" />
    </id>

    <property name="Name">
      <column name="Name" length="500" not-null="true" />
    </property>
    <property name="Address">
      <column name="Location" length="500" not-null="true" />
    </property>
  </class>

</hibernate-mapping>